<appengine-web-app xmlns="http://appengine.google.com/ns/1.0">
  <application>contactsdataapiexample</application>
  <version>1</version>

  <static-files>
    <exclude path="*.jsp"/>
  </static-files>

  <system-properties>
    <!-- The DisableCookieHandler property needs to be set to true when using
         the GData Java client. -->
    <property name="com.google.gdata.DisableCookieHandler" value="true"/>
  </system-properties>

  <resource-files>
    <include path="static/nopicture.gif" />
  </resource-files>

</appengine-web-app>
